{
    "name": "wearemarketing/fizzbuzz-kata",
    "description": "Kata PHP: Fizz Buzz",
    "keywords": ["kata","TDD"],
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^1.0",
        "phpspec/phpspec": "^4.0",
        "leanphp/phpspec-code-coverage": "^4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "WAM Develop Team",
            "email": "develop@wearemarketing.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
